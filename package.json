{
  "name": "binary-clock",
  "version": "1.0.0",
  "description": "A Binary Clock",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack",
    "dev-server": "webpack-dev-server",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "author": "Jonathan Chaidez",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "jest": "^27.5.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "webpack-dev-server": "^4.7.4"
  }
}
